{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/mobile",
      "dest": "app.py"
    },
    {
      "src": "/api/chackout",
      "dest": "app.py"
    },
    {
      "src": "/IMG/(.*)",
      "dest": "/IMG/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
