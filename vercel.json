{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/api/chackout",
      "dest": "/api/chackout.php"
    },
    {
      "src": "/api/success",
      "dest": "/api/success.php"
    },
    {
      "src": "/mobile",
      "dest": "/template/game/mobile.html"
    },
    {
      "src": "/freefire",
      "dest": "/template/game/freefire.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

